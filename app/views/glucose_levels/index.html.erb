<div class="container">
  <h1>Welcome to patient glucose levels report</h1>
</div>



<table class="table table-striped">
  <thead>
    <tr>
      <th scope="col">Levels</th>
      <th scope="col">Created at</th>
    </tr>
  </thead>
  <tbody>
	<% @glucose_levels.each do |gl| %>
    <tr>
      <td><%= gl.glucose %></td>
      <td><%= gl.created_at.strftime("%Y-%m-%d") %></td>
    </tr>
   <% end %>
  </tbody>
</table>

